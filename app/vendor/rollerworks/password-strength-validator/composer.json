{
    "name": "rollerworks/password-strength-validator",
    "description": "Password-strength validator for Symfony",
    "keywords": ["password", "validator", "symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/rollerworks/PasswordStrengthValidator/contributors"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "symfony/polyfill-mbstring": "^1.5.0",
        "symfony/translation": "^3.4.22 || ^4.0 || ^5.0",
        "symfony/validator": "^3.4.22 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.4",
        "php-http/httplug": "^1.1",
        "phpspec/prophecy": "^1.10.3",
        "symfony/config": "^3.4.22 || ^4.0 || ^5.0",
        "symfony/console": "^3.4.22 || ^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\PasswordStrength\\": "src/"
        },
        "exclude-from-classmap": ["test/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\PasswordStrength\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
