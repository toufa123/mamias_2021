{
  "name": "mamias/2020",
  "type": "project",
  "license": "proprietary",
  "description": "MAMIAS webApp 3.0 ",
  "require": {
      "php": "^7.4|^8.0",
      "ext-ctype": "*",
      "ext-gd": "*",
      "ext-iconv": "*",
      "api-platform/core": "^2.6",
      "cocur/slugify": "^4.0",
      "composer/package-versions-deprecated": "1.11.99.4",
      "connectholland/cookie-consent-bundle": "^0.9.6",
      "creof/doctrine2-spatial": "dev-master",
      "creof/geo-parser": "^2.2.1",
      "creof/wkt-parser": "^2.2",
      "doctrine/annotations": "^1.0",
      "doctrine/doctrine-bundle": "^1.12.13 || ^2.3.0",
      "doctrine/doctrine-migrations-bundle": "3.0.1",
      "doctrine/orm": "2.6.6",
      "edsdk/flmngr-server-php": "^1.0",
      "excelwebzone/recaptcha-bundle": "^1.5.26",
      "guzzlehttp/guzzle": "^6.5.5",
      "helios-ag/fm-elfinder-bundle": "^10.1",
      "jms/serializer-bundle": "^3.9.0",
      "jsor/doctrine-postgis": "^1.7",
      "liip/imagine-bundle": "^2.5",
      "mhujer/breadcrumbs-bundle": "1.5.3",
      "nelmio/cors-bundle": "^2.1",
      "ob/highcharts-bundle": "^1.7",
      "orbitale/cms-bundle": "^3.1.1",
      "phpdocumentor/reflection-docblock": "^5.2",
      "phpoffice/phpspreadsheet": "^1.18.0",
      "picoss/sonata-extra-admin-bundle": "^4.0",
      "presta/sitemap-bundle": "^2.5",
      "pugx/autocompleter-bundle": "^1.7",
      "redcode/tree-bundle": "^0.1.0@alpha",
      "rollerworks/password-strength-bundle": "2.2.0",
      "sensio/framework-extra-bundle": "^5.6.1",
      "sonata-project/admin-bundle": "^3.88.0",
      "sonata-project/block-bundle": "^3.17 || ^4.0",
      "sonata-project/cache-bundle": "^2.4.2",
      "sonata-project/classification-bundle": "*",
      "sonata-project/doctrine-orm-admin-bundle": "3.29.*",
      "sonata-project/formatter-bundle": "4.3.0",
      "sonata-project/media-bundle": "^3.28.0",
      "sonata-project/news-bundle": "3.15.0",
      "sonata-project/translation-bundle": "2.4.*",
      "sonata-project/user-bundle": "4.5.*",
      "stof/doctrine-extensions-bundle": "1.5.0",
      "swiftmailer/swiftmailer": "^6.2.5",
      "symfony/asset": "^4.4.20|^5.0.0",
      "symfony/config": "^4.4.20|^5.0",
      "symfony/console": "^4.4.20|^5.0",
      "symfony/expression-language": "^4.4.20",
      "symfony/finder": "^4.4.20",
      "symfony/flex": "^1.9.1",
      "symfony/form": "^4.4.20",
      "symfony/framework-bundle": "^4.4.20",
      "symfony/http-client": "^4.4.20",
      "symfony/intl": "^4.4.20",
      "symfony/mailer": "^4.4.20",
      "symfony/monolog-bundle": "^3.6",
      "symfony/process": "^4.4.20",
      "symfony/property-access": "4.4.20",
      "symfony/property-info": "4.4.20",
      "symfony/proxy-manager-bridge": "4.4.20",
      "symfony/security-bundle": "^4.4.20",
      "symfony/serializer": "4.4.20",
      "symfony/swiftmailer-bundle": "3.4.0",
      "symfony/translation": "^4.4.20",
      "symfony/twig-bundle": "^4.4.20",
      "symfony/validator": "^4.4.20",
      "symfony/web-link": "^4.4.20",
      "symfony/yaml": " ^4.4 || ^5.1",
      "thormeier/breadcrumb-bundle": "^2.2.1",
      "twig/twig": "^2.10 || ^3.0",
      "vich/uploader-bundle": "1.11.*"
  },
  "require-dev": {
      "friendsofphp/php-cs-fixer": "^2.18.4",
      "phpmd/phpmd": "^2.9.1",
      "phpstan/extension-installer": "^1.1",
      "phpstan/phpstan": "^0.12.82",
      "squizlabs/php_codesniffer": "^3.5.8",
      "symfony/browser-kit": "^4.4",
      "symfony/css-selector": "^4.4",
      "symfony/debug-bundle": "^4.4",
      "symfony/dotenv": "4.4.20",
      "symfony/maker-bundle": "^1.30.2",
      "symfony/phpunit-bridge": "^5.3",
      "symfony/stopwatch": "^4.4",
      "symfony/var-dumper": "4.4.4",
      "symfony/web-profiler-bundle": "^4.4"
  },
  "config": {
    "preferred-install": {
        "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "elfinder:install": "symfony-cmd",
      "doctrine:schema:update --force": "symfony-cmd"
    },
    "post-install-cmd": [
        "@auto-scripts",
        "php bin/console doctrine:schema:update --force",
        "php bin/console ckeditor:install --tag=5.0",
        "php bin/console elfinder:install"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "php bin/console doctrine:schema:update --force",
      "php bin/console ckeditor:install",
      "php bin/console elfinder:install"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
      "symfony": {
          "allow-contrib": "true",
          "require": "4.4.20",
          "public-dir": "/public/"
      },
      "phpstan": {
          "includes": [
              "extension.neon"
          ]
      }
  },
    "minimum-stability": "stable",
    "prefer-stable": true
}
